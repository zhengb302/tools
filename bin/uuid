#!/bin/bash

print_usage() {
    echo "Generate some uuids."
    echo "Usage:"
    echo "  uuid           # Generate only one uuid"
    echo "  uuid 5         # Generate five uuids"
}

num=$1

if [ -z "$num" ]; then
    num=1
fi

cat /dev/urandom | od -x | head -$num | awk '{print $2$3"-"$4"-"$5"-"$6$7"-"$8$9}'